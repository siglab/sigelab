{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "systemName": "Sigelab",
        "includeDate": true,
        "theme": "lumen",
        "linenums": true,
        "outputSourceFiles": true,
        "outputSourcePath": true
    }
}
